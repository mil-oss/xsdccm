   <xs:simpleType name="PreReleaseVersionSimpleType">
      <xs:annotation>
         <xs:documentation>A data type for pre-release version information. A pre-release version MAY be denoted by appending a hyphen and a series of dot separated identifiers immediately following the patch version. Identifiers MUST comprise only ASCII alphanumerics and hyphen [0-9A-Za-z-]. Identifiers MUST NOT be empty. Numeric identifiers MUST NOT include leading zeroes. Pre-release versions have a lower precedence than the associated normal version. A pre-release version indicates that the version is unstable and might not satisfy the intended compatibility requirements as denoted by its associated normal version. Examples: 1.0.0-alpha, 1.0.0-alpha.1, 1.0.0-0.3.7, 1.0.0-x.7.z.92.</xs:documentation>
         <xs:appinfo>
            <SimpleType xmlns="urn:security::1.0"
                        name="Pre-Release Version"
                        mapvar="preReleaseVersion"/>
         </xs:appinfo>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:pattern value="-[0-9A-Za-z-]{0,24}"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="PythonDevelopmentVersionSimpleType">
