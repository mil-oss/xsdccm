          <xs:element ref="StandardLicenseHeader" minOccurs="0"/>
          <xs:element ref="LicenseText" minOccurs="1"/>
          <xs:element ref="StandardLicenseTemplate" minOccurs="1"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
