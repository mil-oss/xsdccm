{
  "name": "ng4-xsdccm-guide",
  "version": "1.0.0",
  "scripts": {
    "build": "del-cli public/xsdccm/js/app && webpack-dev-server --inline --progress --port 8080 --config webpack.config.dev.js",
    "build:prod": "del-cli public/xsdccm/js/app && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'assets/app/**/*.js' 'assets/app/**/*.ngfactory.ts' 'assets/app/**/*.js.map' 'assets/app/**/*.shim.ts' 'assets/app/**/*.ngsummary.json' 'assets/app/**/*.ngstyle.ts' 'public/xsdccm/js/app' 'assets/polyfills.ngsummary.json'",
    "serve": "lite-server"
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@angular/upgrade": "^6.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.3.3",
    "vkbeautify": "^0.99.3",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.10.2",
    "@types/core-js": "0.9.36",
    "@types/node": "^6.0.45",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.5.0",
    "del-cli": "^0.2.1",
    "html-loader": "^0.4.5",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lite-server": "^2.3.0",
    "raw-loader": "^0.5.1",
    "typescript": "2.7.2",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.2"
  }
